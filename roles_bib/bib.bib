@incollection{books/sp/map2005/BordiniHV05,
  author    = {Rafael H. Bordini and
               Jomi Fred H{\"u}bner and
               Renata Vieira},
  title     = {Jason and the Golden Fleece of Agent-Oriented Programming},
  booktitle = {Multi-Agent Programming},
  year      = {2005},
  pages     = {3-37},
  crossref  = {books/sp/map05},
  bibsource = {DBLP, http://dblp.uni-trier.de},
  annote = {
    Jason provides a communication infrastructure for agents and an interpreter
    for the AgentSpeak(L)~\cite{conf/maamaw/Rao96} language. Messages may be
    delivered using KQML~\cite{todo} or FIPA-ACL~\cite{todo}. The choice is 
    governed by the \emph{infrastructure} being used. By itself, Jason does
    not support roles. Together, the add-ons S-Moise+~\cite{todo} and 
    J-Moise+~\cite{todo} implement the conceptual model 
    MOISE~\cite{journals/ijaose/HubnerSB07}, thus supporting roles and 
    centrally managed organizations.
    
    The implementation, integrated with the editor jEdit, is of 
    high-quality and the development effort is ongoing.
  }
}

